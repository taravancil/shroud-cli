{
  "name": "shroud-cli",
  "version": "0.2.0",
  "description": "An filesystem-based CLI password manager",
  "bin": {
    "shroud": "build/bin/shroud"
  },
  "scripts": {
    "build": "mkdir -p build && gulp compile",
    "prepublish": "standard && npm run build",
    "start": "gulp",
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taravancil/shroud-cli"
  },
  "author": {
    "name": "Tara Vancil",
    "email": "tbvanc@gmail.com"
  },
  "keywords": [
    "password",
    "password manager",
    "secrets",
    "secret store",
    "encryption"
  ],
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "copy-paste": "^1.3.0",
    "inquirer": "^1.2.3",
    "minimist": "^1.2.0",
    "shroud": "^0.3.0"
  }
}
