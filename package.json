{
  "name": "shroud-cli",
  "version": "0.3.0",
  "description": "An filesystem-based CLI password manager",
  "bin": {
    "shroud": "./lib/cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "scripts": {
    "build": "mkdir -p build",
    "prepare": "npm run test && mkdir -p build",
    "test": "prettier --check lib/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taravancil/shroud-cli"
  },
  "author": {
    "name": "Tara Vancil",
    "email": "contact@taravancil.com"
  },
  "keywords": [
    "password",
    "password manager",
    "secrets",
    "secret store",
    "encryption"
  ],
  "license": "MIT",
  "devDependencies": {
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "copy-paste": "^1.3.0",
    "inquirer": "^1.2.3",
    "minimist": "^1.2.0",
    "shroud": "^0.6.1"
  }
}
